{
  "expo": {
    "name": "Calimali",
    "slug": "calimali-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/calimali-icon.png",
    "scheme": "calimaliapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/calimali-icon-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1D23"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/calimali-icon-adaptive.png",
        "backgroundColor": "#1E1D23"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.calimaliapp",
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/calimali-icon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
